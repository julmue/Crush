{-# LANGUAGE WarnOpenExpr #-}

module StdLib.Logic
    ( -- logic
    , tru
    , fls
    , not 
    , and
    , or 
    , imp
    , iff
    , if
      -- equality
    , eqbool
    ) where

tru = \t.\f.t
fls = \t.\f.f
not = \p.\t.\f.p f t
and = \p.\q.p q p
or = \p.\q.p p q
imp = \p.\q.or (not p) q
iff = \p.\q.and (imp p q) (imp q p)
eqbool = iff
if = \p.\t.\f.p t f
